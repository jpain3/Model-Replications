{UTF-8}
Opt Goal=
	("Organizational Capacity (C)"-0.5)^2
	~	
	~		|

"Average Capability Lifetime (τ L)"=
	60
	~	Week
	~		|

"Minimum Organizational Capacity (C min)"=
	0.1
	~	Dmnl
	~		|

"Normal Fraction of Relief Effort Impacting Capacity (Nf CE)"=
	0.1
	~	Dmnl [0,1,0.001]
	~		|

"Normal Productivity (NPdy)"=
	2
	~	people/manhour
	~		|

"Time to Change Allocation (τ E)"=
	2
	~	Week
	~		|

"People Receiving Relief (P)"=
	"Effort Allocated to Relief (E r)"*"Productivity of Relief Effort (Pdy)"
	~	people/Week
	~		|

α=
	0.5
	~	Dmnl
	~		|

"Time to Invest in Capacity (τ C)"=
	6
	~	Week
	~		|

"Capability Erosion (C E)"=
	"Organizational Capacity (C)"/"Average Capability Lifetime (τ L)"
	~	Dmnl/Week
	~		|

"Change in Effort Allocation (CE r)"=
	("Indicated Effort Allocation to Relief (IE r)"-"Effort Allocated to Relief (E r)")/\
		"Time to Change Allocation (τ E)"
	~	manhours/Week/Week
	~		|

"Effect of Stress (S E)"=
	1-α*("Gap in Relief Effort (GE)"/"Total Effort (ET)")
	~	Dmnl
	~		|

"Effort Allocated to Capacity Improvement (E I)"=
	"Total Effort (ET)"-"Effort Allocated to Relief (E r)"
	~	manhours/Week
	~		|

"Effort Allocated to Relief (E r)"= INTEG (
	"Change in Effort Allocation (CE r)",
		500)
	~	manhours/Week
	~		|

"Fraction of Relief Effort Impacting Capacity (f CE)"=
	"Normal Fraction of Relief Effort Impacting Capacity (Nf CE)"*"Effect of Stress (S E)"
	~	Dmnl
	~		|

"Gap in People Receiving Relief (GP)"=
	"People Requiring Relief (P*)"-"People Receiving Relief (P)"
	~	people/Week
	~		|

"Gap in Relief Effort (GE)"=
	"Gap in People Receiving Relief (GP)"/"Productivity of Relief Effort (Pdy)"
	~	manhours/Week
	~		|

"Indicated Capacity (C*)"=
	"Reference Effort to Improvement (RE I)"/"Total Effort (ET)"
	~	Dmnl
	~		|

"Indicated Capacity (IC)"=
	max("Minimum Organizational Capacity (C min)",min(1,"Organizational Capacity (C)"*("Effort Allocated to Capacity Improvement (E I)"\
		+"Relief Effort Impacting Capacity (E C)")/"Reference Effort to Improvement (RE I)"\
		))
	~	Dmnl
	~		|

"Indicated Effort Allocation to Relief (IE r)"=
	max("Minimum Effort Allocated to Relief (E Rmin)",min("Total Effort (ET)",("Effort Allocated to Relief (E r)"\
		+"Gap in Relief Effort (GE)")))
	~	manhours/Week
	~		|

"Investment in Capability (C I)"=
	("Indicated Capacity (IC)"-"Organizational Capacity (C)")/"Time to Invest in Capacity (τ C)"
	~	Dmnl/Week
	~		|

"Minimum Effort Allocated to Relief (E Rmin)"=
	10
	~	manhours/Week
	~		|

"Organizational Capacity (C)"= INTEG (
	"Investment in Capability (C I)"-"Capability Erosion (C E)",
		0.5)
	~	Dmnl
	~		|

"People Requiring Relief (P*)"=
	1000
	~	people/Week
	~		|

"Productivity of Relief Effort (Pdy)"=
	"Normal Productivity (NPdy)"*("Organizational Capacity (C)"/"Indicated Capacity (C*)"\
		)^2
	~	people/manhour
	~		|

"Reference Effort to Improvement (RE I)"=
	500
	~	manhours/Week
	~		|

"Relief Effort Impacting Capacity (E C)"=
	"Effort Allocated to Relief (E r)"*"Fraction of Relief Effort Impacting Capacity (f CE)"
	~	manhours/Week
	~		|

"Total Effort (ET)"=
	1000
	~	manhours/Week
	~		|

